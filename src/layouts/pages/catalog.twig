{% extends theme %}

{% block main %}
	{% include '../components/Goods.twig' with goods %}


	{% include '../components/VideoCard.twig' with process %}
{% endblock %}

{% block modal %}
	{% for good in goods.list %}
		{% include '../components/Modal.twig' with {
			component: 'FormSelect',
			data: orderForm|merge({ goodId: good.id }),
			modalId: good.id
		} %}
	{% endfor %}
{% endblock %}
